{
"parameters": {
    "webAppName": {
      "type": "string",
      "defaultValue": "MyMvcApp-<uniqueString>",
      "metadata": {
        "description": "Name of the web app."
      }
    },
    "appServicePlanName": {
      "type": "string",
      "defaultValue": "MyMvcAppPlan-<uniqueString>",
      "metadata": {
        "description": "Name of the App Service Plan."
      }
    },
    "location": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Location for all resources."
      }
    },
    "sku": {
      "type": "string",
      "defaultValue": "F1",
      "metadata": {
        "description": "The SKU of the App Service Plan."
      }
    }
  }
}
